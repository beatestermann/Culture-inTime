
<table class="ui celled table">
  <thead><tr> 
    <th>Property</th>
    <th>Count</th>
    <th>URI</th>
    <th></th>
  </tr></thead>
  <tbody>
    <% properties.each do |prop| %>
    <tr>
      <td data-label="Property"> <%= prop["l_sample"]["value"].capitalize %></td>
      <td data-label="Count"> <%= prop["count"]["value"] %></td>
      <td data-label="URI"> <%= prop["p"]["value"] %></td>
      <td><button class="ui button">Add to layout</button> </td>
    </tr>
    <% end %>
  </tbody>
</table>